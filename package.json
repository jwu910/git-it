{
  "bin": {
    "gitstarted": "index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "name": "git-started",
  "version": "0.0.1",
  "description": "Browse repositories by users. Quickly fork and clone.",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "commit": "commit",
    "cz": "git-cz",
    "format": "prettier --write \"**/*.js\"",
    "lint:fix": "eslint --config .eslintrc.json ./ --fix",
    "lint": "eslint --config .eslintrc.json ./",
    "prepublishOnly": "npm run build",
    "start": "node index.js",
    "test": "npm run lint",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwu910/git-started.git"
  },
  "keywords": [
    "git",
    "fork",
    "project",
    "repository",
    "clone"
  ],
  "author": "Joshua Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwu910/git-started/issues"
  },
  "homepage": "https://github.com/jwu910/git-started#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "commander": "^2.19.0",
    "dotenv": "^6.2.0",
    "graphql-request": "^1.8.2",
    "prompts": "^2.0.3",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt-cli": "^7.5.0",
    "@commitlint/travis-cli": "^7.5.2",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "pretty-quick": "^1.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
